{
  "expo": {
    "platforms": [
      "android",
      "ios"
    ],
    "name": "Chat",
    "slug": "Chat",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSBluetoothPeripheralUsageDescription": "We need Bluetooth to detect nearby users.",
        "NSLocationWhenInUseUsageDescription": "We need location access to find nearby users."
      },
      "bitcode": false,
      "bundleIdentifier": "com.duongnguyen1263.Chat",
      "entitlements": {
        "aps-environment": "development"
      }
    },
    "android": {
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "BLUETOOTH",
        "BLUETOOTH_ADMIN",
        "WAKE_LOCK",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.BLUETOOTH_CONNECT",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.CAMERA",
        "android.permission.INTERNET",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.RECORD_AUDIO",
        "android.permission.SYSTEM_ALERT_WINDOW",
        "android.permission.WAKE_LOCK",
        "android.permission.BLUETOOTH",
        "android.permission.VIBRATE",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.USE_FULL_SCREEN_INTENT"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.duongnguyen1263.Chat",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "@videosdk.live/expo-config-plugin",
      [
        "@config-plugins/react-native-webrtc",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#4f8bff",
          "sounds": [
            "./assets/sounds/join.mp3",
            "./assets/sounds/calling.mp3"
          ],
          "mode": "production",
          "android": {
            "channelId": "default",
            "priority": "high"
          },
          "ios": {
            "sound": true,
            "badge": true,
            "alert": true
          }
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.256923005911-m2j0sqfikfi21dor3mjo0rlnehe7p4a1"
        }
      ],
      [
        "react-native-fbsdk-next",
        {
          "appID": "3128658277362764",
          "clientToken": "727f260d6676a9b07d206e7f42a9a92c",
          "displayName": "ChappAt",
          "scheme": "fb3128658277362764",
          "advertiserIDCollectionEnabled": false,
          "autoLogAppEventsEnabled": false,
          "isAutoInitEnabled": true,
          "iosUserTrackingPermission": "This identifier will be used to deliver personalized ads to you."
        }
      ],
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "517519e7-47e5-475f-b0a6-d22cbebbcf68"
      }
    },
    "owner": "duongnguyen1263"
  }
}
